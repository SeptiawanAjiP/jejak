{
    "name": "dewakoding/jejak",
    "description": "A Laravel package for tracking user behavior and analytics.",
    "type": "library",
    "keywords": ["laravel", "tracking", "analytics", "user-behavior", "footprint", "jejak"],
    "license": "MIT",
    "authors": [
        {
            "name": "Septiawan Aji Pradana",
            "email": "dev.dewakoding@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "jenssegers/agent": "^2.6",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dewakoding\\Jejak\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dewakoding\\Jejak\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dewakoding\\Jejak\\JejakServiceProvider"
            ],
            "aliases": {
                "Jejak": "Dewakoding\\Jejak\\Facades\\Jejak"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
